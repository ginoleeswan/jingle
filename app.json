{
  "expo": {
    "name": "jingle",
    "slug": "jingle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/jingle-icon.png",
    "splash": {
      "image": "./app/assets/jingle-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/jingle-icon-android.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/jingle-favicon.png"
    }
  }
}
